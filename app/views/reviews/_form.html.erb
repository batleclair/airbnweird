<%= simple_form_for([Booking.where(user_id: current_user.id, friend_id:params[:id])[0], @review],
    data: {
      insert_in_list_target: "form",
      action: "submit->insert-in-list#send"
    }) do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
